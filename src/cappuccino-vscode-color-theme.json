	"tokenColors": [
		{
			"name": "Text",
			"scope": ["text"],
			"settings": {
				"foreground": "#d6e3ff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"comment.line",
				"comment.line.double-slash",
				"comment.line.double-dash",
				"comment.line.number-sign",
				"comment.line.percentage",
				"comment.line.character",
				"comment.block",
				"comment.block.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#006591"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.character",
				"constant.character.escape",
				"constant.language",
				"constant.other",
				"string.interpolated",
				"string.regexp",
				"support.constant",
				"markup.underline.link",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#9dd852"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity",
				"entity.name",
				"entity.name.type",
				"entity.name.tag",
				"entity.name.section",
				"entity.other.inherited-class",
				"support.class",
				"support.type",
				"variable.other.object",
				"markup.heading",
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.id",
				"storage.type"
			],
			"settings": {
				"foreground": "#007bcc"
			}
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#ff4f75"
			}
		},
		{
			"name": "Invalids",
			"scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
			"settings": {
				"fontStyle": "bold strikethrough"
			}
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "keyword.control", "keyword.other", "storage.modifier"],
			"settings": {
				"foreground": "#a04ce6"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"string.quoted.single",
				"string.quoted.double",
				"string.quoted.triple",
				"string.quoted.other",
				"string.unquoted",
				"string.other"
			],
			"settings": {
				"foreground": "#00a669"
			}
		},
		{
			"name": "Imports and Attributes",
			"scope": [
				"support",
				"support.other.namespace",
				"entity.name.type.namespace",
				"variable.other.readwrite.alias",
				"storage.modifier.package",
				"storage.modifier.import"
			],
			"settings": {
				"foreground": "#524cca"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"support.other",
				"variable",
				"variable.parameter",
				"variable.language",
				"variable.other",
				"entity.name.variable",
				"entity.other.attribute-name",
				"variable.other.object.property",
				"meta.arrow variable.other.object",
				"meta.object.member",
				"support.type.property-name",
				"meta.at-rule",
				"meta.method",
				"constant.other.key"
			],
			"settings": {
				"foreground": "#96ccff"
			}
		},
		{
			"name": "Operators and Punctuation",
			"scope": [
				"punctuation",
				"keyword.operator",
				"meta.brace.round",
				"meta.brace.square",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#fff1b2"
			}
		}
	]
}

