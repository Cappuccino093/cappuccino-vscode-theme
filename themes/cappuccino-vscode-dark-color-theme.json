	"tokenColors": [
		{
			"name": "Text",
			"scope": [
				"source",
				"text",
				"text.html.derivative",
				"text.html.markdown",
				"punctuation.definition.list_item.markdown",
				"entity.name.command.shell",
				"string.unquoted.argument.shell",
				"variable.css",
				"variable.scss"
			],
			"settings": {
				"foreground": "#BBDEFB"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#F44336"
			}
		},
		{
			"name": "Italic",
			"scope": ["markup.quote"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.definition.block",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"storage.type.function.arrow",
				"keyword.operator.comparison",
				"meta.brace.round",
				"text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown",
				"keyword.operator.type.annotation",
				"keyword.operator.assignment",
				"meta.brace"
			],
			"settings": {
				"foreground": "#BDBDBD"
			}
		},
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#607D8B"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.other",
				"variable.language.this",
				"storage.modifier",
				"storage.type.class"
			],
			"settings": {
				"foreground": "#BA68C8"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"storage.type.annotation",
				"tag.decorator entity.name.tag",
				"tag.decorator punctuation.definition.tag",
				"meta.decorator"
			],
			"settings": {
				"foreground": "#FFC107"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"markdown.heading",
				"markup.heading",
				"markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown",
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types",
				"support.type",
				"entity.other.attribute-name.class",
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#2962FF"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.block",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.method",
				"meta.class-method entity.name.function",
				"variable.function.constructor",
				"entity.name.module",
				"variable.import.parameter.",
				"variable.other.class.",
				"meta.definition.method",
				"entity.other.ng-binding-name",
				"storage.type.ts"
			],
			"settings": {
				"foreground": "#E57373"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"string constant.other.placeholder",
				"variable.parameter",
				"variable.language",
				"entity.other.attribute-name",
				"string.other.link.title.markdown",
				"markup.raw.block.fenced.markdown",
				"variable.language.fenced.markdown",
				"punctuation.section.class.end",
				"variable.language.fenced.markdown",
				"markup.table",
				"meta.object-literal.key",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#64B5F6"
			}
		},
		{
			"name": "Important Variables",
			"scope": [
				"support.other.variable",
				"string.other.link",
				"punctuation.definition.fenced.markdown",
				"markup.raw.block.fenced.markdown",
				"markup.raw.block",
				"string.other.link.description.title.markdown",
				"markup.quote punctuation.definition.blockquote.markdown",
				"markup.bold markup.italic",
				"markup.italic markup.bold",
				"markup.quote markup.bold",
				"markup.bold markup.italic string",
				"markup.italic markup.bold string",
				"markup.quote markup.bold string",
				"markup.bold",
				"markup.bold string",
				"markup.italic",
				"text.html.markdown markup.inline.raw.markdown",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#3F51B5"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key string.unquoted.label",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#81C784"
			}
		},
		{
			"name": "Special Strings",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"constant.other.color",
				"keyword.other.unit",
				"punctuation.definition.constant",
				"string.regexp",
				"constant.character.escape",
				"*url*",
				"*link*",
				"*uri*",
				"markup.underline",
				"constant.other.reference.link.markdown",
				"expression.ng variable.other"
			],
			"settings": {
				"foreground": "#DCE775"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#8BC34A"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#F44336"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#2196F3"
			}
		}
	]
}

